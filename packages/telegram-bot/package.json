{
  "name": "@celebrum-ai/telegram-bot",
  "version": "0.1.0",
  "description": "Telegram Bot TypeScript interfaces and utilities for Celebrum AI",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./handlers": {
      "types": "./dist/handlers/index.d.ts",
      "import": "./dist/handlers/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsup --watch",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@celebrum-ai/db": "workspace:*",
    "@celebrum-ai/shared": "workspace:*",
    "@cloudflare/workers-types": "^4.20250620.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "oxlint": "^1.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "telegram",
    "bot",
    "typescript",
    "cloudflare",
    "workers",
    "arbitrage"
  ]
}