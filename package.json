{
  "name": "arb-edge",
  "version": "0.1.0",
  "description": "A Cloudflare Worker project for Arbitrage Edge.",
  "keywords": [
    "cloudflare-worker",
    "arbitrage",
    "edge-computing"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "wrangler deploy --dry-run --outdir=dist",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "NO_COLOR=1 vitest run",
    "test:watch": "NO_COLOR=1 vitest",
    "test:coverage": "NO_COLOR=1 vitest run --coverage",
    "lint": "biome lint ./src",
    "format": "biome format ./src ./tests --write"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.26",
    "@colors/colors": "^1.6.0",
    "@types/node": "^22.15.12",
    "@vitest/coverage-istanbul": "3.1.3",
    "@vitest/coverage-v8": "^3.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vitest": "^3.1.0",
    "vitest-mock-extended": "^3.1.0",
    "wrangler": "^4"
  },
  "pnpm": {
    "overrides": {
      "@colors/colors": "1.6.0"
    }
  },
  "dependencies": {
    "ccxt": "^4.4.78",
    "grammy": "^1.36.1",
    "itty-router": "^5.0.18",
    "p-ratelimit": "^1.0.1",
    "winston": "^3.13.0",
    "zod": "^3.24.4"
  }
}
